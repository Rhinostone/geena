{
    "_comment" : "Please, don't touch this.",
    "defEnv" : "prod",
    "logLevels" : [
        "silly",
        "trace",
        "info",
        "debug",
        "warn",
        "error"
    ],
    "defExt" : "",
    "mountPath" : "{executionPath}/bundles",
    "globalTmpPath"   : "{executionPath}/tmp",
    "logsPath"  : "{executionPath}/logs",
    "{bundle}" : {
        "{env}" : {
            "bundlesPath"   : "{executionPath}/bundles",
            "bundlePath"    : "{bundles}/{bundle}",
            "modelsPath"    : "{bundles}/{bundle}/models",
            "libPath"       : "{bundles}/{bundle}/lib",
            "releases"      : "{executionPath}/releases",
            "sources"       : "{executionPath}/src",
            "tmpPath"       : "{bundles}/{bundle}/tmp",
            "protocol"      : "http",
            "port" : {
                "http" : 82,
                "https" : 443,
                "ftp" : 21,
                "ssh" : 22
            },
            "server" : {
                "response" : {
                    "header" : {
                        "accept-language": "en-US,en;q=0.8,fr;q=0.6"
                    }
                },
                "webroot" : "/"
            },
            "encoding" : "utf8",
            "files" :{
                "app"       : "app.json",
                "routing"   : "routing.json",
                "connectors" : "connectors.json",
                "settings"   : "settings.json",
                "statics"   : "statics.json",
                "views"      : "views.json"
            },
            "contexts" : {
                "paths" : {}
            }
        }
    },
    "system": {},
    "log" : [
        { "path" : "/logs/gina" }
    ]
}
