{
  "_comment": "Please, don't touch this.",
  "defEnv": "prod",
  "logLevels": [
    "silly",
    "trace",
    "info",
    "debug",
    "warn",
    "error"
  ],
  "defExt": "",
  "mountPath": "{executionPath}/bundles",
  "globalTmpPath": "{executionPath}/tmp",
  "logsPath": "{executionPath}/logs",
  "{bundle}": {
    "{env}": {
      "projectPath": "{executionPath}",
      "bundlesPath": "{executionPath}/bundles",
      "bundlePath": "{bundlesPath}/{bundle}",
      "connectorsPath": "{frameworkDir}/core/connectors",
      "modelsPath": "{bundlesPath}/{bundle}/models",
      "libPath": "{bundlesPath}/{bundle}/lib",
      "releases": "{executionPath}/releases",
      "sources": "{executionPath}/src",
      "tmpPath": "{bundlesPath}/{bundle}/tmp",      
      "template": false,
      "server": {
        "engine": "express",
        "webroot": "/",
        "webrootAutoredirect": true,
        "credentials": {
          "_comment": "SSL Credentials: private key & certificate samples to use for https & http/2 protocols",
          "privateKey": "{projectPath}/ssl/server.key",
          "certificate": "{projectPath}/ssl/server.crt",
          "passphrase": "1234567"
      },
        "response": {
          "header": {
            "accept-language": "en-US,en;q=0.8,fr;q=0.6",
            "X-Powered-By": "Gina I/O - v{version}"
          }
        }
      },
      "encoding": "utf8",
      "files": [
        "app",
        "connectors",
        "routing",
        "settings",
        "statics",
        "views",
        "watchers"
      ],
      "filesOld": {
        "_comment": "app.json is handled in core/config.js",
        "connectors": "connectors.json",
        "routing": "routing.json",
        "settings": "settings.json",
        "statics": "statics.json",
        "views": "views.json",
        "watchers": "watchers.json"
      },
      "contexts": {
        "paths": {}
      }
    }
  },
  "system": {},
  "log": [
    {
      "path": "/logs/gina"
    }
  ]
}
