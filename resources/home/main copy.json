{
    "frameworks": {
        "{release}": [ "{version}" ]
    },
    "def_framework": "{version}",
    "envs": {
        "{release}": [ "dev" ]
    },
    "def_env": {
      "{release}": "dev"
    },
    "dev_env": {
        "{release}": "dev"
    },
    "protocols": {
        "_comment": "e.g.:  http/1.1, http/2.0, net",
        "{release}": [  "http/1.1", "http/2.0" ]
    },
    "def_protocol": {
        "{release}": "http/1.1"
    },
    "schemes": {
        "_comment": "e.g.:  http, https",
        "{release}": [  "http", "https" ]
    },
    "def_scheme": {
        "{release}": "http"
    },
    "cultures": {
        "_comment": "e.g.:  en-US, fr-FR",
        "{release}": [  "en-US", "fr-FR" ]
    },
    "def_culture": {
        "{release}": "en-US"
    }
}